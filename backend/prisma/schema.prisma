
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

// Simple User model - just username for now
model User {
  id        String   @id @default(cuid())
  username  String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
